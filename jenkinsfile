pipeline {
    agent any
    stages {
        stage('Ejecutar Pipeline con parámetros') {
            steps {
                script {
                    // Definir los parámetros para el job de tipo FreeStyle
                    def PORT = '8083'
                    // Llamar al job de tipo FreeStyle con los parámetros definidos
                    build job: 'JavaWebAppParam', parameters: [
                        string(name: 'PORT', value: '8083')
                                            ]
                }
            }
        }
        stage('Ejecutar Pipeline sin parámetros') {
            steps {
                // Llamar a la pipeline sin parámetros
                build 'Daily shutdown containers'
            }
        }
    }
